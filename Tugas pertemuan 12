#include <iostream>
#include <conio.h>
using namespace std;

float luas(float s)
{
float luas;
luas=s*s;
return luas;
}
main(void)
{
float sisi;
float L;
cout<< " \tProgram Menghitung Luas Persegi"<<endl<<endl;
